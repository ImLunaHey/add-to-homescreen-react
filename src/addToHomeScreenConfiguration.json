{
  "appID": "ServicePortal",
  "debug": false,
  "logging": false,
  "modal": false,
  "mandatory": false,
  "autoStart": true,
  "skipFirstVisit": false,
  "minPageViews": 0,
  "startDelay": 1,
  "lifespan": 15,
  "displayPace": 1440,
