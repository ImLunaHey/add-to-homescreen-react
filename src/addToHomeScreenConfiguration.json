    "animated",
    "d-flex"
  ],
  "showClass": "d-flex",
  "hideClass": "d-none",
  "customPromptElements": {
    "title": ".ath-banner-title",
    "body": ".ath-banner",
    "logo": ".ath-prompt-logo",
    "cancel": ".btn-cancel",
    "install": ".btn-install"
  },
  "customPromptPlatformDependencies": {
    "native": {
      "showClasses": [
        "fadeInUp",
        "right-banner"
